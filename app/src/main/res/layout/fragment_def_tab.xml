<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.daren.myapplication.DefTab">

    <TextView
        android:id="@+id/netResult"
        android:layout_width="322dp"
        android:layout_height="373dp"
        android:layout_below="@+id/getXML"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.652" />

    <Button
        android:id="@+id/getXML"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editText"
        android:layout_toEndOf="@+id/editText"
        android:layout_toRightOf="@+id/editText"
        android:text="Get Definition" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/netResult"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/netResult"
        android:layout_marginTop="43dp"
        android:ems="10"
        android:hint="Word"
        android:inputType="textPersonName" />

</RelativeLayout>
